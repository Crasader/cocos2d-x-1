3.
CREAT_FUNC(HelloWorldLayer)相当于：
HelloWorldLayer *HelloWorldLayer::create()
{
 HelloWorldLayer *layer= new HelloWorldLayer();
if(layer && layer->init())
{
layer->autorelease();
return layer;

}

CC_SAFE_DELETE(layer);
return NULL;

}


4.Layer::create():
Layer *Layer::create()
{
    Layer *ret = new Layer();
    if (ret && ret->init())
    {
        ret->autorelease();
        return ret;
    }
    else
    {
        CC_SAFE_DELETE(ret);
        return nullptr;
    }
}

